{% paginate search.results by 4 %}
  {% if search.performed %}
    <h1>Here's your for something</h1>
    {% for result in search.results %}
      {% if result.object_type == "page" %}
        <a href="{{ result.url }}">{{ result.title }}</a>
        {% elseif result.object_type == "article" %}
        <a href="{{ result.url }}">{{ result.title }}</a>
      {% else %}
        <a href="{{ result.url }}">{{ result.title }}</a>
      {% endif %}
    {% endfor %}
  {% else %}
    <h1>Try search for something</h1>
  {% endif %}
{% endpaginate %}